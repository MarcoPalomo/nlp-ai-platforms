{
    "model_name": "mistral-7b-instruct",
    "model_version": "v0.1",
    "model_type": "text-generation",
    "max_length": 4096,
    "temperature": 0.7,
    "top_p": 0.9,
    "top_k": 50,
    "repetition_penalty": 1.1,
    "do_sample": true,
    "pad_token_id": 2,
    "eos_token_id": 2,
    "bos_token_id": 1,
    "tokenizer": {
      "model_max_length": 4096,
      "padding_side": "left",
      "truncation_side": "left",
      "add_special_tokens": true
    },
    "generation_config": {
      "max_new_tokens": 512,
      "min_new_tokens": 1,
      "use_cache": true,
      "num_beams": 1,
      "early_stopping": false
    },
    "device_map": "auto",
    "torch_dtype": "float16",
    "load_in_8bit": false,
    "load_in_4bit": false,
    "trust_remote_code": true,
    "low_cpu_mem_usage": true,
    "batch_size": 1,
    "max_batch_size": 4,
    "timeout": 300,
    "logging": {
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  }